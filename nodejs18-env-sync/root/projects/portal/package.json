{
  "name": "portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node test.js",
    "yolo": "./run.sh",
    "verbose": "DEBUG=ioredis:* ./run.sh",
    "test": "npx jest --verbose --testMatch='**/src/__test__/unit/**/?(*.)+(spec|test).js'",
    "test:integration": "npx jest --detectOpenHandles --verbose --testMatch='**/src/__test__/integration/**/?(*.)+(spec|test).js'",
    "test:watch":"npx jest --watchAll --verbose --testMatch='**/src/__test__/unit/**/?(*.)+(spec|test).js'",
    "test:coverage": "npx jest --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "async-mutex": "^0.3.2",
    "body-parser": "^1.19.1",
    "bootstrap": "^5.1.3",
    "client-certificate-auth": "^0.3.0",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "~1.4.4",
    "datatables": "^1.10.18",
    "debug": "~2.6.9",
    "dotenv": "^11.0.0",
    "ejs": "^3.1.6",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "http-errors": "~1.6.3",
    "https": "^1.0.0",
    "ioredis": "^4.28.5",
    "jquery": "^3.6.0",
    "jszip": "3.10.1",
    "morgan": "~1.9.1",
    "mv": "^2.1.1",
    "node-esapi": "0.0.1"
  },
  "devDependencies": {
    "jest": "^29.4.3",
    "prettier": "^2.8.8"
  }
}
